
@model IEnumerable<ODMS.Models.ViewModel.DBhouseemployeeiVm>



<div class="panel panel-default">
    <div class="panel-heading">
        <div class="clearfix ">
            @*@Html.ActionLink("Create New", "Create", null, new { @class = "btn btn-primary pull-left" })*@

            <span id="Exportbtn" class="pull-right"> </span>
        </div>
    </div>
    <div class="panel-body">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.EmpCode)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Name)
                    </th>

                    <th>
                        @Html.DisplayNameFor(model => model.Distribution)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.EmpType)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.LoginUserId)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Active)
                    </th>
                    <th>Actions</th>
                </tr>
            </thead>

            <tbody>


                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.EmpCode)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Name)
                        </td>

                        <td>
                            @Html.DisplayFor(modelItem => item.Distribution)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.EmpType)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.LoginUserId)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Active)
                        </td>
                        <td>
                            <div class="dropdown">
                                <button class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
                                    Action
                                    <span class="caret"></span>
                                </button>
                                <ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
                                    <li>@Html.ActionLink("Details", "Details", new { id = item.Id }, new { target = "_blank" })</li>
                                    <li role="separator" class="divider"></li>
                                    <li>@Html.ActionLink("Edit", "Edit", new { id = item.Id }, new { target = "_blank" })</li>
                                    <li role="separator" class="divider"></li>
                                    <li>@Html.ActionLink("Delete", "Delete", new { id = item.Id }, new { target = "_blank" })</li>

                                </ul>
                            </div>
                        </td>
                    </tr>
                }

        </table>
    </div>
</div>
